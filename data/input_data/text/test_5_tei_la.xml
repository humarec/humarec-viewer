	<text xml:id="Paul_transcript" n="Paul">
		<group>
			<text xml:id="Paul_transcript_1Cor" n="1 Cor">
				<body>
					<pb xml:id="Gr-Z_0011-00379_155v" facs="#Gr-Z_0011-00379_155v" n="155v"/>
					<p>
						<lb facs="#Gr-Z_0011-00379_155v_la_line_21" n="21" xml:id="Gr-Z_0011-00379_155v_la_lb_21"/>
						<hi rend="red">nos · Incipit episto</hi>
						<lb facs="#Gr-Z_0011-00379_155v_la_line_22" n="22" xml:id="Gr-Z_0011-00379_155v_la_lb_22"/><hi rend="red">la
						ad corinthios</hi>
						<lb facs="#Gr-Z_0011-00379_155v_la_line_23" n="23" xml:id="Gr-Z_0011-00379_155v_la_lb_23"/>
						<hi rend="red">prima :</hi>
						<lb facs="#Gr-Z_0011-00379_155v_la_line_24" n="24" xml:id="Gr-Z_0011-00379_155v_la_lb_24"/>
						<milestone unit="verse" n="1:1"/><hi rend="red">P</hi>aulus vocatus apostolus
						<lb facs="#Gr-Z_0011-00379_155v_la_line_25" n="25" xml:id="Gr-Z_0011-00379_155v_la_lb_25"/>
						christi iesu per volunta
						<lb facs="#Gr-Z_0011-00379_155v_la_line_26" n="26" xml:id="Gr-Z_0011-00379_155v_la_lb_26"/>
						tem dei et sosthenes frater · <milestone unit="verse" n="1:2"/> ecclesiae </p>

					<pb xml:id="Gr-Z_0011-00379_156r" facs="#Gr-Z_0011-00379_156r" n="156r"/>
					<p>
						<lb facs="#Gr-Z_0011-00379_156r_la_line_1" n="1" xml:id="Gr-Z_0011-00379_156r_la_lb_1"/>
						dei quae est corinthi
						<lb facs="#Gr-Z_0011-00379_156r_la_line_2" n="2" xml:id="Gr-Z_0011-00379_156r_la_lb_2"/>
						sanctificatis in christo iesu ·
						<lb facs="#Gr-Z_0011-00379_156r_la_line_3" n="3" xml:id="Gr-Z_0011-00379_156r_la_lb_3"/>
						vocatis sanctis · cum omnibus
						<lb facs="#Gr-Z_0011-00379_156r_la_line_4" n="4" xml:id="Gr-Z_0011-00379_156r_la_lb_4"/>
						qui invocant nomen domini nostri
						<lb facs="#Gr-Z_0011-00379_156r_la_line_5" n="5" xml:id="Gr-Z_0011-00379_156r_la_lb_5"/>
						iesu christi ~ in omni loco
						<lb facs="#Gr-Z_0011-00379_156r_la_line_6" n="6" xml:id="Gr-Z_0011-00379_156r_la_lb_6"/>
						ipsorum et nostro · <lb facs="#Gr-Z_0011-00379_156r_la_line_7" n="7" xml:id="Gr-Z_0011-00379_156r_la_lb_7"/>
