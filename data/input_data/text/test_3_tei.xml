	<text xml:id="Paul_transcript" n="Paul">
		<group>
			<text xml:id="Paul_transcript_1Cor" n="1 Cor">
				<body>
					<pb xml:id="Gr-Z_0011-00379_155v" facs="#Gr-Z_0011-00379_155v" n="155v"/>
					<p>
						<cb xml:id="Gr-Z_0011-00379_155v_gr"/>
						<lb facs="#Gr-Z_0011-00379_155v_gr_line_24" n="24" xml:id="Gr-Z_0011-00379_155v_gr_lb_24"/>
						ΠΡΟΣΚΟΡΙΝΘΙΟΥΣΠΡΩΤΗΣ
						<lb facs="#Gr-Z_0011-00379_155v_gr_line_25" n="25" xml:id="Gr-Z_0011-00379_155v_gr_lb_25"/>Παῦλος
						κλητὸς ἀπόστολος
							<lb facs="#Gr-Z_0011-00379_155v_gr_line_26" n="26" xml:id="Gr-Z_0011-00379_155v_gr_lb_26"/><abbr>ιη</abbr><expan>ἰησοῦ</expan>
						<abbr>χυ</abbr><expan>χριστοῦ</expan> διὰ θελήματος
							<abbr>θυ</abbr><expan>θεοῦ</expan> · καὶ
						<lb facs="#Gr-Z_0011-00379_155v_gr_line_27" n="27" xml:id="Gr-Z_0011-00379_155v_gr_27"/>σωσθένης
						ὁ ἀδελφὸς <milestone unit="verse" n="1:2"/> τῆ ἐκ <cb
							xml:id="Gr-Z_0011-00379_155v_la"/>
						<lb facs="#Gr-Z_0011-00379_155v_la_line_21" n="21" xml:id="Gr-Z_0011-00379_155v_la_lb_21"/>
						nos · Incipit episto
						<lb facs="#Gr-Z_0011-00379_155v_la_line_22" n="22" xml:id="Gr-Z_0011-00379_155v_la_lb_22"/>la
						ad corinthios
						<lb facs="#Gr-Z_0011-00379_155v_la_line_23" n="23" xml:id="Gr-Z_0011-00379_155v_la_lb_23"/>
						prima :
						<lb facs="#Gr-Z_0011-00379_155v_la_line_24" n="24" xml:id="Gr-Z_0011-00379_155v_la_lb_24"/>
						Paulus vocatus apostolus
						<lb facs="#Gr-Z_0011-00379_155v_la_line_25" n="25" xml:id="Gr-Z_0011-00379_155v_la_lb_25"/>
						christi iesu per volunta
						<lb facs="#Gr-Z_0011-00379_155v_la_line_26" n="26" xml:id="Gr-Z_0011-00379_155v_la_lb_26"/>
						tem dei et sosthenes frater · <milestone unit="verse" n="1:2"/> ecclesiae
							<cb xml:id="Gr-Z_0011-00379_155v_ar"/>
						<lb facs="#Gr-Z_0011-00379_155v_ar_line_23" n="23" xml:id="Gr-Z_0011-00379_155v_ar_lb_23"/>
						و هذه رِسَاله بولس الحواري
						<lb facs="#Gr-Z_0011-00379_155v_ar_line_24" n="24" xml:id="Gr-Z_0011-00379_155v_ar_lb_24"/>
						الاولى الي اهل قرنثه
						<lb facs="#Gr-Z_0011-00379_155v_ar_line_25" n="25" xml:id="Gr-Z_0011-00379_155v_ar_lb_25"/>
						من بولس الرسول المُدعَى حواري
						<lb facs="#Gr-Z_0011-00379_155v_ar_line_26" n="26" xml:id="Gr-Z_0011-00379_155v_ar_lb_26"/>
						بيسوع المسيح باردة الله //
